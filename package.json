{
  "name": "rdm",
  "version": "0.0.5",
  "description": "Markdown partial processor",
  "author": "muji <noop@xpm.io>",
  "repository": {
    "type": "git",
    "url": "https://github.com/freeformsystems/rdm"
  },
  "bugs": {
    "url": "https://github.com/freeformsystems/rdm/issues",
    "email": "muji <noop@xpm.io>"
  },
  "licenses": [{
    "type": "MIT",
    "url": "https://github.com/freeformsystems/rdm/blob/master/LICENSE"
  }],
  "keywords": [
    "cli",
    "readme",
    "markdown",
    "html",
    "partial"
  ],
  "dependencies": {
    "async": "~0.2.10",
    "ttycolor": "~0.8.4",
    "cli-interface": "~1.0.5",
    "cli-command": "~0.8.10",
    "cli-util": "~1.1.11",
    "marked": "~0.3.1"
  },
  "devDependencies": {
    "mocha": "~1.17.0",
    "chai": "~1.9.0",
    "istanbul": "~0.2.4"
  },
  "engine": [ "node >= 0.10.21" ],
  "scripts": {
    "prepublish": "./bin/rdm --force",
    "test": "istanbul cover _mocha -- -u bdd --recursive --bail --reporter list -A test/unit/"
  },
  "directories": {
    "bin": "./bin"
  },
  "readme" : {
    "title": "rdm(1)",
    "pedantic": true,
    "includes": "readme",
    "gfm": true,
    "branch": "master",
    "links": "links.md",
    "toc": "Table of Contents",
    "order": true,
    "base": "https://github.com/freeformsystems/rdm",
    "partial": [
      "# first",
      "## child1",
      "## child2",
      "# last",
      "* item1\n* item2\n* item3"
    ]
  }
}
