{
  "name": "rdm",
  "version": "0.0.6",
  "description": "Markdown partial processor",
  "author": "muji <noop@xpm.io>",
  "repository": {
    "type": "git",
    "url": "https://github.com/freeformsystems/rdm"
  },
  "bugs": {
    "url": "https://github.com/freeformsystems/rdm/issues",
    "email": "muji <noop@xpm.io>"
  },
  "licenses": [{
    "type": "MIT",
    "url": "https://github.com/freeformsystems/rdm/blob/master/LICENSE"
  }],
  "keywords": [
    "cli",
    "readme",
    "markdown",
    "html",
    "partial"
  ],
  "dependencies": {
    "async": "~0.2.10",
    "ttycolor": "~0.8.4",
    "circular": "~1.0.2",
    "cli-interface": "~1.0.5",
    "cli-command": "~0.8.10",
    "cli-util": "~1.1.11",
    "marked": "~0.3.1"
  },
  "devDependencies": {
    "mocha": "~1.17.0",
    "chai": "~1.9.0",
    "istanbul": "~0.2.4"
  },
  "engine": [ "node >= 0.10.21" ],
  "scripts": {
    "prepublish": "./bin/rdm --force",
    "build": "./bin/rdm --force --debug",
    "test": "istanbul cover _mocha -- -u bdd --recursive --bail --reporter list -A test/unit/"
  },
  "directories": {
    "bin": "./bin"
  },
  "rdm" : {
    "title": "rdm(1)",
    "pedantic": true,
    "includes": "doc/readme",
    "require": "lib",
    "gfm": true,
    "branch": "master",
    "links": "links.md",
    "toc": "Table of Contents",
    "order": false,
    "base": "https://github.com/freeformsystems/rdm",
    "partial": [
      {
        "property": "description"
      },
      {
        "include": "introduction.md"
      },
      {
        "include": "partial.md"
      },
      "## Configuration\n\nThis document was generated with the following configuration (see [package.json](/package.json)):",
      {
        "property": "rdm",
        "stringify": true,
        "format": "```json\n%s\n```"
      },
      "## Defaults\n\n",
      {
        "require": "defaults.js",
        "type": "code",
        "language": "javascript"
      },
      {
        "include": "meta.md"
      },
      "## Middleware",
      {
        "include": "middleware.md"
      },
      "The `inspect` middleware is shown below:",
      {
        "require": "middleware/inspect.js",
        "type": "code",
        "language": "javascript"
      },
      {
        "property": {"middleware": ["inspect"]},
        "type": "code",
        "language": "json"
      },
      "## Usage",
      {
        "script": "rdm --help",
        "type": "code"
      },
      "## Manual",
      {
        "env": {
          "cli_toolkit_help_style": "markdown",
          "cli_toolkit_help_markdown_title": true,
          "cli_toolkit_help_markdown_header": "###"
        },
        "script": "rdm --help",
        "pedantic": false
      },
      {
        "include": "license.md"
      },
      {
        "include": "../../LICENSE"
      }
    ]
  }
}
