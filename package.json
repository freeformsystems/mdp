{
  "name": "mdp",
  "version": "0.1.0",
  "description": "Markdown partial processor",
  "author": "muji <noop@xpm.io>",
  "repository": {
    "type": "git",
    "url": "https://github.com/freeformsystems/mdp"
  },
  "bugs": {
    "url": "https://github.com/freeformsystems/mdp/issues",
    "email": "muji <noop@xpm.io>"
  },
  "licenses": [{
    "type": "MIT",
    "url": "https://github.com/freeformsystems/mdp/blob/master/LICENSE"
  }],
  "keywords": [
    "cli",
    "readme",
    "markdown",
    "html",
    "partial"
  ],
  "dependencies": {
    "async": "~0.2.10",
    "ttycolor": "~0.8.4",
    "circular": "~1.0.2",
    "cli-interface": "~1.0.5",
    "cli-command": "~0.8.10",
    "cli-util": "~1.1.11",
    "marked": "~0.3.1",
    "moment": "~2.5.1"
  },
  "devDependencies": {
    "mocha": "~1.17.0",
    "chai": "~1.9.0",
    "istanbul": "~0.2.4"
  },
  "engine": [ "node >= 0.10.21" ],
  "scripts": {
    "prepublish": "./bin/mdp --force",
    "build": "./bin/mdp --force --debug",
    "test": "istanbul cover _mocha -- -u bdd --recursive --bail --reporter list -A test/unit/"
  },
  "directories": {
    "bin": "./bin"
  },
  "mdp" : {
    "title": {
      "ref": "name",
      "format": "%s(1)"
    },
    "pedantic": true,
    "include": "doc/readme",
    "require": "lib",
    "gfm": true,
    "branch": "master",
    "links": "links.md",
    "toc": "Table of Contents",
    "order": false,
    "base": "https://github.com/freeformsystems/mdp",
    "partial": [
      {
        "ref": "description"
      },
      {
        "inc": ["introduction.md", "features.md"]
      }
    ]
  }
}
